<% include partials/header.ejs %>


<div class="container_home">
  <% homeJournal_content.forEach(function(post) { %>
    <div>
      <h1><%= post.title %></h1>
      <p class="para">
        
        <% shortenString = post.content.substring(0, 200).concat('...'); %>
        <%= shortenString %>
        <a href="/post/ + <%=post.title %>">Read more...</a>
      </p>
    </div>
  <% }); %>

</div>




<% include partials/footer.ejs %>
